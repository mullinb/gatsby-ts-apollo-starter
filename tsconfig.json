{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonJs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "target": "es5",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "skipLibCheck": true,
    "lib": ["dom", "es6", "esnext.asynciterable"],
    "jsx": "react",
    "outDir": "../dist"
  },
  "include": ["./src/**/*", "gatsby-browser.js"],
  "exclude": ["node_modules"]
}
